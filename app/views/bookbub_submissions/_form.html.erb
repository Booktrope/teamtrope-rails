<%= simple_form_for(@bookbub_submission) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :project %>
    <%= f.input :submitted_by_id %>
    <%= f.input :author %>
    <%= f.input :title %>
    <%= f.input :asin %>
    <%= f.input :asin_linked_url %>
    <%= f.input :current_price %>
    <%= f.input :num_stars %>
    <%= f.input :num_reviews %>
    <%= f.input :avg_rating %>
    <%= f.input :num_pages %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
