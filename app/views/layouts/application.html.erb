<body class="single single-projects postid-6187 logged-in no-customize-support custom-background group-blog masthead-fixed singular no-js" >
  <div id="page" class="hfeed site">
    <%= render 'layouts/header' %>
    <!-- Execute this after the nav header is defined -->
    <%= javascript_include_tag('twentyfourteen/functions.js') %>

    <div id="main" class="site-main">
      <div class="page">
        <div class='padder'>
          <% flash.each do | message_type, message | %>
            <div class="alert alert-<%= message_type %>"><%= message %></div>
          <% end %>
          <%= yield %>
        </div>
      </div>
    </div>
    <%= debug(params) if Rails.env.development? %>
    <%= render 'layouts/footer' %>
  </div><!-- #page -->

  <div style="display:none"></div>
