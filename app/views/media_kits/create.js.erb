console.log(<%=@last_errors%>);
<% if @media_kit.persisted? %>
$('#upload_<%=params[:unique_id]%>').hide();

<% if ! @updated_file.nil? %>
$('#media_kit_<%=@updated_file%>_s3_uploader').html('<span><%=params[:filename] %> was successfully uploaded.</span>');

// Add the media kit type
if(media_kit_uploaded_types) {
    console.log('Adding <%=@updated_file%> to uploaded types list.');
    media_kit_uploaded_types.push('<%=@updated_file%>');
    requiredMediaKitDocumentUnlockSubmit();
} else {
    console.error('media_kits_uploaded_types is undefined!  Cannot track uploaded book.')
}

<% end %>

<% else %>
$('#upload_<%=params[:unique_id]%> div.progress').removeClass('active progress-striped').addClass('progress-danger');
<% end %>
