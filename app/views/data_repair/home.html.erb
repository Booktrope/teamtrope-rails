<% provide(:title, "Data Repair") %>
<div class="row">
  <h1>Data Repair</h1>
  <div class="col-md-4">
    <%= form_tag({ action: :print_price }, multipart: true) do %>
      <div class="form-group">
        <%= label_tag "Upload File" %>
        <%= file_field_tag 'upload_file', class: "form-control" %>
      </div>
      <%= submit_tag "submit", class: "btn btn-default" %>
    <% end %>
  </div>
</div>
