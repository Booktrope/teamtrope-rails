<% provide(:title, "New Project") %>
<div class="padder">
  <div class="page" id="blog-page" role="main">
  <h1>New Project</h1>
  <%= form_for @project do | f | %>
    <%= f.label :final_title %><br />
		<%= f.text_field :final_title %><br /><br />
	
		<h2>Genres</h2>
		<%= f.label :genre_ids, "Genres" %><br />
		<%= f.collection_select(:genre_ids, Genre.all, :id, :name, { include_hidden: false }, { multiple: true } ) %><br /><br />
		<%= f.submit "Save Project" %>
  <% end %>
  </div>
</div>