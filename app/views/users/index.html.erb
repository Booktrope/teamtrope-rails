<% provide(:title, "Members") %>
<h1>Members</h1>
<% @users.each do | user | %>
	<div>
		<%= link_to image_tag(user.avatar.url(:thumb)), user %>
		<%= link_to user.name, user %> | 
		<%= link_to "Delete", user,
		          method: :delete,
              data: { confirm: 'Are you sure you want to delete this comment?' } %>
	</div>
<% end %>