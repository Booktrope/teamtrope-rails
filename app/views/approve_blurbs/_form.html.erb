<%= simple_form_for(@approve_blurb) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :project %>
    <%= f.input :blurb_notes %>
    <%= f.input :blurb_approval_decision %>
    <%= f.input :blurb_approval_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
